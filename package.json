{
  "name": "li-assistant",
  "version": "1.0.0",
  "description": "LinkedIn job application automation with local LLM intelligence",
  "type": "module",
  "private": true,
  "scripts": {
    "login": "tsx src/commands/login.ts",
    "search": "tsx src/cli.ts search",
    "apply": "tsx src/cli.ts apply",
    "status": "tsx src/cli.ts status",
    "list": "tsx src/cli.ts list",
    "rank": "tsx src/cli.ts rank",
    "clear-cache": "tsx src/cli.ts clear-cache",
    "test": "tsx --test tests/**/*.test.ts",
    "test:all": "node tests/run-all-tests.cjs",
    "test:learning": "node tests/learning-system/run-learning-tests.cjs",
    "dashboard:dev": "concurrently --kill-others \"tsx src/dashboard/server.ts\" \"node scripts/wait-for-server.js && vite\"",
    "dashboard:build": "vite build",
    "dashboard:serve": "tsx src/dashboard/server.ts",
    "dashboard:prod": "npm run dashboard:build && npm run dashboard:serve"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "better-sqlite3": "^11.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "playwright": "^1.46.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "yaml": "^2.4.5",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.14.10",
    "@types/yargs": "^17.0.32",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.16.0",
    "typescript": "^5.6.3",
    "vite": "^7.1.12"
  }
}
